<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sun-web-app PUBLIC "-//Sun Microsystems, Inc.//DTD GlassFish Application Server 3.0 Servlet 3.0//EN" "http://www.sun.com/software/appserver/dtds/sun-web-app_3_0-0.dtd">
<sun-web-app error-url="">
  <class-loader delegate="true"/>
  <jsp-config>
    <property name="keepgenerated" value="true">
      <description>Keep a copy of the generated servlet class' java code.</description>
    </property>
  </jsp-config>
  <context-root>/1Projeto</context-root>
  <servlet>
    <servlet-name>InserirUsuario</servlet-name>
    <servlet-class>controle.InserirUsuario</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>InserirUsuario</servlet-name>
    <url-pattern>/InserirUsuario</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>EfetuarLogin</servlet-name>
    <servlet-class>controle.EfetuarLogin</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>EfetuarLogin</servlet-name>
    <url-pattern>/EfetuarLogin</url-pattern>
  </servlet-mapping>
  <session-config>
    <session-timeout>
            30
        </session-timeout>
  </session-config>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</sun-web-app>
